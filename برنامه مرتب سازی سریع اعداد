#include <stdio.h>
#include <conio.h>
#include <iostream>

using namespace std;


void quicksort(int a[], int s, int e) {
        if(s>=e)
            return;
        int q = partition(a,s,e);
        quicksort(a, s, q-1);
        quicksort(a, q+1, e);

    }

int partition(int a[], int s, int e) {
        int x = a[e];
        int i = s-1;
        for(int j = s;j<e;++j){
            if(a[j]<=x){
                ++i;
                int temp = a[i];
                a[i]= a[j];
                a[j]=temp;
            }
        }
        ++i;
        int temp = a[i];
        a[i]= a[e];
        a[e]=temp;
        return i;
    }

int main() {

    int size;
    cin>>size;
    int a[];
    for(int l=0;l<size;l++)
    {
    cin>>a[l];
    }

    quicksort(a,,size);

    for (int i = ;  i < size; ++i) {
        cout<< a[i];
    }


    return 0;
}
